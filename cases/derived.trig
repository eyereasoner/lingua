# Proving a derived rule.

PREFIX string: <http://www.w3.org/2000/10/swap/string#>
PREFIX lingua: <http://www.w3.org/2000/10/swap/lingua#>
PREFIX var: <http://www.w3.org/2000/10/swap/var#>
PREFIX : <#>

:Alice a :Cat.
:Minka a :Cat.
:Charly a :Dog.

_:bng_1 lingua:forward _:bng_2.

GRAPH _:bng_1 {
    var:cat a :Cat.
}

GRAPH _:bng_2 {
    _:bng_3 lingua:forward _:bng_4.
}

GRAPH _:bng_3 {
    var:dog a :Dog.
}

GRAPH _:bng_4 {
    :test :is true.
}

# query
_:bng_5 lingua:answer _:bng_5.

GRAPH _:bng_5 {
    :test :is var:result.
}
